{
	"extends": ["../../.eslintrc.json"],
	"ignorePatterns": ["!**/*"],
	"overrides": [
		{
			"files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
			"rules": {}
		},
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {
				"@typescript-eslint/no-non-null-assertion": "warn",
				"@typescript-eslint/no-inferrable-types": "warn"
			}
		},
		{
			"files": ["*.js", "*.jsx"],
			"rules": {}
		}
	]
}
